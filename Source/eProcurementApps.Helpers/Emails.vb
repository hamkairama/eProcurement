    Public Shared Sub OpenFile(fileName As String, fileByte As Byte())
        Try
            Dim tempFolder As String = System.IO.Path.GetTempPath()
            fileName = System.IO.Path.Combine(tempFolder, fileName)
            File.WriteAllBytes(fileName, fileByte)
            Process.Start(fileName)
        Catch exp As Exception
            Throw exp
        End Try
    End Sub

    Public Shared Sub OpenFile(page As System.Web.UI.Page, fileName As String, fileByte As Byte())
        Try
            Dim mimeType As String = String.Empty
            If fileName.Contains(".gif") Then
                mimeType = "image/gif"
            ElseIf fileName.Contains(".jpg") Then
                mimeType = "image/jpeg"
            ElseIf fileName.Contains(".jpeg") Then
                mimeType = "image/jpeg"
            ElseIf fileName.Contains(".jpe") Then
                mimeType = "image/jpeg"
            ElseIf fileName.Contains(".png") Then
                mimeType = "image/png"
            ElseIf fileName.Contains(".bmp") Then
                mimeType = "image/bmp"
            ElseIf fileName.Contains(".pdf") Then
                mimeType = "application/pdf"
            ElseIf fileName.Contains(".doc") Then
                mimeType = "application/doc"
            ElseIf fileName.Contains(".docx") Then
                mimeType = "application/doc"
            ElseIf fileName.Contains(".xls") Then
                mimeType = "application/xlsx"
            ElseIf fileName.Contains(".gif") Then
                mimeType = "application/xls"
            ElseIf fileName.Contains(".txt") Then
                mimeType = "application/txt"
            End If

            page.Response.Clear()
            page.Response.AddHeader("content-disposition", String.Format("attachment;filename=""{0}""", fileName))
            page.Response.ContentType = mimeType
            page.Response.BinaryWrite(fileByte)
            page.Response.Flush()
            page.Response.[End]()
        Catch exp As Exception
            Throw exp
        End Try
    End Sub

