    Public Shared Function NameOfMonth(month As Integer) As String
        Try
            Dim result As String = String.Empty
            If month = 1 Then
                result = "Januari"
            ElseIf month = 2 Then
                result = "Februari"
            ElseIf month = 3 Then
                result = "Maret"
            ElseIf month = 4 Then
                result = "April"
            ElseIf month = 5 Then
                result = "Mei"
            ElseIf month = 6 Then
                result = "Juni"
            ElseIf month = 7 Then
                result = "Juli"
            ElseIf month = 8 Then
                result = "Agustus"
            ElseIf month = 9 Then
                result = "September"
            ElseIf month = 10 Then
                result = "Oktober"
            ElseIf month = 11 Then
                result = "November"
            ElseIf month = 12 Then
                result = "Desember"
            End If
            Return result
        Catch exp As Exception
            Throw exp
        End Try
    End Function
