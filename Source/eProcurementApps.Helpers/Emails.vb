    Public Shared Function SendEmail(from As String, [to] As String, bcc As String, subject As String, body As String, attachment As Attachment,
        smtpAddress As String) As String
        Dim message As String = String.Empty
        Try
            If Not EmailValidator.EmailIsValid(from) Then
                message = "Kirim email gagal. Format email ""from"" tidak valid"
            ElseIf Not EmailValidator.EmailIsValid([to]) Then
                message = "Kirim email gagal. Format email ""to"" tidak valid"
            ElseIf Not EmailValidator.EmailIsValid(bcc) Then
                message = "Kirim email gagal. Format email ""to"" tidak valid"
            Else
                Dim msg As New MailMessage()
                msg.[To].Add([to])
                msg.Bcc.Add(bcc)
                msg.Subject = subject
                msg.From = New MailAddress(from, "Administrator")
                msg.IsBodyHtml = True
                msg.Body = body
                msg.Attachments.Add(attachment)
                Dim smtp As New SmtpClient(smtpAddress)
                smtp.Send(msg)
            End If
            Return "Kirim email sukses"
        Catch exp As Exception
            Throw exp
        End Try
    End Function

